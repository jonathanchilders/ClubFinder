<body>
	
	<div class="container">
		<!-- h1 through h6 are headers. The higher the number, the smaller the header -->
		<!-- p means paragraph -->
		<center>
		<div class="jumbotron">
			<h1>ClubFinder</h1>
			<p>Find the club for you</p>
		</div>
		</center>
		<center>
			<ol>
	    	{{#each homepage_options}}
	      		<ul>
	      			<button class="button">
	      				<span>
	      					<a href="#{{id}}">{{name}}</a>
	      				</span>
	      			</button>
	      		</ul>
	    	{{/each}}
	    	</ol>
	    </center>

	</div>

	<script src="https://code.jquery.com/jquery.js"></script>
	<script src="/js/bootstrap.min.js"></script>
</body>
</html>
